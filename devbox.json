{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "go@1.23",
    "golangci-lint@1.62",
    "gum@0.14.5"
  ],
  "shell": {
    "init_hook": [
      "echo 'Welcome to devbox!' > /dev/null"
    ],
    "scripts": {
      "lint": "${DEVBOX_PROJECT_ROOT}/scripts/lint $*",
      "test": "${DEVBOX_PROJECT_ROOT}/scripts/test $*"
    }
  }
}

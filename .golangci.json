{
    "linters": {
        "enable": [
            "goconst",
            "dupl",
            "nilerr",
            "nilnil",
            "nonamedreturns",
            "tagalign",
            "unconvert",
            "unparam",
            "usestdlibvars",
            "wastedassign"
        ]
    },
    "issues": {
        "exclude-rules": [
            {
                "path": "(.+)_test\\.go",
                "linters": [
                    "dupl"
                ]
            }
        ]
    }
}